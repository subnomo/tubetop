{
  "name": "tubetop",
  "version": "0.2.0",
  "description": "A YouTube music player, for your desktop.",
  "main": "build/app.js",
  "repository": "https://github.com/subnomo/tubetop",
  "author": "Alex Taylor <alex@alext.xyz>",
  "license": "MIT",
  "scripts": {
    "start": "node fuse",
    "build": "node fuse build",
    "dist": "node fuse build && electron-builder",
    "dist:all": "node fuse build && electron-builder -wl --x64 --ia32",
    "test": "jest --coverage",
    "test:nocov": "jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "build": {
    "artifactName": "${name}-${version}-${os}-${arch}-setup.${ext}",
    "files": [
      "build/**/*"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/renderer/**/*.{ts,tsx}",
      "!src/renderer/**/constants.{ts,tsx}",
      "!src/renderer/**/styles.{ts,tsx}",
      "!src/renderer/**/*.test.{ts,tsx}",
      "!src/renderer/utils/**/*.{ts,tsx}",
      "!src/renderer/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src/renderer"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/src/renderer/utils/shim.ts",
      "<rootDir>/src/renderer/utils/setupTests.ts"
    ],
    "testMatch": [
      "**/tests/*.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "^4.6.2",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.0.4",
    "@types/prop-types": "^15.5.2",
    "@types/rc-slider": "^8.2.3",
    "@types/react": "^16.3.13",
    "@types/react-autosuggest": "^9.3.4",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/redux-immutable": "^3.0.38",
    "@types/uuid": "^3.4.3",
    "@types/whatwg-fetch": "^0.0.33",
    "coveralls": "^3.0.1",
    "electron": "^2.0.8",
    "electron-builder": "^20.11.1",
    "electron-devtools-installer": "^2.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fuse-box": "3.2.2",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.5.0",
    "immutable": "^3.8.2",
    "invariant": "^2.2.4",
    "lodash": "^4.17.13",
    "material-ui": "1.0.0-beta.25",
    "material-ui-icons": "1.0.0-beta.17",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react": "^16.3.2",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-persist-immutable": "^4.3.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.6",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4",
    "ytdl-core": "^0.20.2"
  }
}
