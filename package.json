{
  "name": "tubetop",
  "version": "0.1.0",
  "description": "A YouTube music player, for your desktop.",
  "main": "dist/app.js",
  "repository": "https://github.com/subnomo/tubetop",
  "author": "Alex Taylor <alex@subnomo.me>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test:nocov": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/renderer/**/*.{ts,tsx}",
      "!src/renderer/**/constants.{ts,tsx}",
      "!src/renderer/**/styles.{ts,tsx}",
      "!src/renderer/**/*.test.{ts,tsx}",
      "!src/renderer/utils/**/*.{ts,tsx}",
      "!src/renderer/index.tsx",
      "!src/renderer/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "mapCoverage": true,
    "moduleDirectories": [
      "node_modules",
      "src/renderer"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/src/renderer/utils/shim.ts",
      "<rootDir>/src/renderer/utils/setupTests.ts"
    ],
    "testMatch": [
      "**/tests/*.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/history": "^4.6.2",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.86",
    "@types/node": "^8.0.53",
    "@types/rc-slider": "^8.2.0",
    "@types/react": "^16.0.25",
    "@types/react-autosuggest": "^9.3.1",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.10",
    "@types/redux-immutable": "^3.0.38",
    "electron-devtools-installer": "^2.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "fuse-box": "^2.5.0-beta.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "typescript": "^2.6.2",
    "uglify-js": "^3.2.0"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "electron": "1.7.9",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "rc-slider": "^8.4.1",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^5.4.0",
    "redux-persist-immutable": "^4.3.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.2.3",
    "ulid": "^2.2.2",
    "ytdl-core": "^0.18.3"
  }
}
